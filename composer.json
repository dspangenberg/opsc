{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": ["laravel", "framework"],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "cviebrock/eloquent-sluggable": "^11.0",
    "inertiajs/inertia-laravel": "2.x-dev",
    "inertiaui/modal": "^0.11.1",
    "laragear/webauthn": "^3.1",
    "laravel/framework": "^11.9",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.9",
    "maize-tech/laravel-markable": "^2.3",
    "matanyadaev/laravel-eloquent-spatial": "^4.4",
    "mpdf/mpdf": "^8.2",
    "mpdf/qrcode": "^1.2",
    "plank/laravel-mediable": "^6.2",
    "ploi/ploi-php-sdk": "^1.9",
    "rikudou/euqrpayment": "^1.4",
    "spatie/laravel-data": "^4.11",
    "spatie/laravel-settings": "^3.4",
    "spatie/laravel-typescript-transformer": "^2.5",
    "spatie/temporary-directory": "^2.3",
    "stancl/tenancy": "dev-master",
    "tightenco/ziggy": "^2.0",
    "torann/hashids": "^3.0",
    "webklex/laravel-imap": "5.3.0.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.14",
    "barryvdh/laravel-ide-helper": "^3.2",
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^3.0",
    "laravel-lang/http-statuses": "^3.8",
    "laravel-lang/lang": "^15.8",
    "laravel/breeze": "^2.2",
    "laravel/pail": "^1.1",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.1",
    "pestphp/pest": "^3.5",
    "pestphp/pest-plugin-laravel": "^3.0",
    "phpstan/phpstan": "^2.0",
    "soloterm/solo": "^0.3.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tenancy-for-laravel/v4"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mysticseagull/momentum-modal"
    }
  ],
  "autoload": {
      "files":[
          "app/Helpers/Opsc.php"
  ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan lang:update",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
