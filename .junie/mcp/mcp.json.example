{
    "mcpServers": {
        "laravel-boost": {
            "command": "/path/to/php",
            "args": [
                "/path/to/project/artisan",
                "boost:mcp"
            ]
        },
        "herd": {
            "command": "/path/to/php",
            "args": [
                "/path/to/herd-mcp.phar"
            ],
            "env": {
                "SITE_PATH": "/path/to/project"
            }
        }
    }
}
